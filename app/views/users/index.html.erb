<div class = "page-header">
  <h1>Photogram Users</h1>
</div>

<table class = "table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
      </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><a href = "/users/<%= user.id %>"><%= user.id %></td>
        <td><a href = "/users/<%= user.id %>"><%= user.username %></td>
      </tr>
    <% end %>
  </tbody>
</table>
